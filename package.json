{
  "name": "api-doc-generator",
  "version": "0.0.1",
  "description": "REST API Documentation generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/dzekevis/api-doc-generator"
  },
  "main": "api-doc-generator.js",
  "bin": {
    "api-doc-generator": "src/bin.js"
  },
  "maintainers": [
    {
      "name": "sergeidzekevich",
      "email": "sdzekevich@dzekevis.com"
    }
  ],
  "author": "Siarhei Dzekevich <sdzekevich@dzekevis.com>",
  "keywords": [
    "nodejs",
    "rest",
    "documentation"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "gulpfile.js",
    "src/*",
    "dist/*"
  ],
  "ignore": [
    ".editorconfig",
    ".eslintrc",
    ".gitignore",
    ".sasslintrc",
    ".travis.yml",
    "package.json",
    "src",
    "test"
  ],
  "license": "MIT",
  "readme": "README.md",
  "dependencies": {
    "dox": "0.9.0",
    "handlebars": "4.0.8",
    "yargs": "7.0.2",
    "fs-extra": "2.1.2",
    "babel-register": "6.24.1",
    "babel-preset-es2015": "6.24.1"
  },
  "devDependencies": {
    "normalize.css": "6.0.0",
    "milligram": "1.3.0",
    "gulp": "3.9.1",
    "gulp-sass": "3.1.0",
    "gulp-concat": "2.6.1",
    "gulp-minify": "0.0.15",
    "gulp-clean-css": "3.0.4",
    "gulp-clean": "0.3.2",
    "gulp-babel": "6.1.2",
    "banner-cli": "0.11.0",
    "npm-run-all": "4.0.2",
    "rimraf": "2.6.1",
    "editorconfig-tools": "^0.1.1",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-config-airbnb-base": "11.1.3",
    "sass-lint": "1.10.2",
    "nyc": "10.3.2",
    "ava": "0.19.1"
  },
  "scripts": {
    "lint-styles": "sass-lint -c .sasslintrc src --verbose --no-exit",
    "lint-scripts": "eslint src -c airbnb-base",
    "lint-config": "editorconfig-tools check .",
    "lint": "npm run lint-styles && npm run lint-scripts && npm run lint-config",
    "ava": "nyc ava",
    "banner": "banner-cli dist/*.css",
    "gulp": "gulp",
    "clean": "rimraf dist",
    "build": "run-s clean gulp banner",
    "start": "run-p build",
    "test": "run-s build lint ava"
  }
}
